{
  "name": "automart",
  "version": "1.0.0",
  "extends": [
    "plugin:jasmine"
  ],
  "description": "An online market place for automobiles where  users can sell their cars or buy from trusted dealerships or private sellers.",
  "main": "src/index.js",
  "nyc": {
    "require": [
      "@babel/register"
    ],
    "reporter": [
      "lcov",
      "text"
    ],
    "sourceMap": false,
    "instrument": false
  },
  "scripts": {
    "start": "nodemon --exec babel-node src/index.js",
    "test-init": "jasmine init",
    "test": "cross-env NODE_ENV=test nyc babel-node node_modules/jasmine/bin/jasmine.js --config=src/spec/support/jasmine.json",
    "coverage": "cross-env NODE_ENV=test nyc report --reporter=text-lcov | coveralls"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/minega25/AutoMart.git"
  },
  "author": "Minega Shyaka Patrick",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/minega25/AutoMart/UI/issues"
  },
  "homepage": "https://github.com/minega25/AutoMart#readme",
  "dependencies": {
    "@hapi/joi": "^15.0.3",
    "bcrypt": "^3.0.6",
    "body-parser": "^1.19.0",
    "config": "^3.1.0",
    "cors": "^2.8.5",
    "express": "^4.17.0",
    "express-async-errors": "^3.1.1",
    "jsonwebtoken": "^8.5.1",
    "lodash": "^4.17.11",
    "moment": "^2.24.0",
    "request": "^2.88.0",
    "swagger-jsdoc": "^3.2.9",
    "swagger-ui-express": "^4.0.5",
    "uuid": "^3.3.2",
    "winston": "^3.2.1"
  },
  "devDependencies": {
    "@babel/cli": "^7.4.4",
    "@babel/core": "^7.4.5",
    "@babel/node": "^7.4.5",
    "@babel/preset-env": "^7.4.5",
    "@babel/register": "^7.4.4",
    "babel-plugin-istanbul": "^5.1.4",
    "coveralls": "^3.0.3",
    "cross-env": "^5.2.0",
    "eslint": "^5.16.0",
    "eslint-config-airbnb-base": "^13.1.0",
    "eslint-plugin-import": "^2.17.3",
    "eslint-plugin-jasmine": "^2.10.1",
    "heroku": "^7.24.4",
    "jasmine": "^3.4.0",
    "jasmine-console-reporter": "^3.1.0",
    "jasmine-node": "^3.0.0",
    "node-mocks-http": "^1.7.5",
    "nyc": "^14.1.1",
    "supertest": "^4.0.2",
    "travis-cli": "^1.0.9"
